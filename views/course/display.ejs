<%- include("../header.ejs") %>


<br>
<section style="background-color: antiquewhite;">
<h1 class=" text-center bg-info text-light" style="margin-top: 70px;">Course Display</h1>
<div class="container">
    <table class="table table-bordered text-center">
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
            <th>Course</th>
            <th>Status</th>
            <th>Message</th>
            <th>Action</th>
        </tr>
        <% c.forEach((item) => { %>
            <tr>
                <td><%= item.id %></td>
                <td><%= item.name %></td>
                <td><%= item.email %>@</td>
                <td><%= item.course %></td>
                <td><%= item.status %></td>
                <td><%= item.comment %></td>
                <td>
                    <a href="/courseView/<%= item.id %>" class="btn btn-info">View</a>
                    <a href="/courseEdit/<%= item.id %>" class="btn btn-success">Edit</a>
                    <a href="/courseDelete/<%= item.id %>" class="btn btn-danger">Delete</a>
                </td>
            </tr>
      <%  }) %>

      
    </table>

    <center>
        <a href="/home" class="btn btn-info">Back</a>
    </center>
</div>
</section>



<%- include("../footer.ejs") %>